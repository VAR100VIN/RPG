    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ competence.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ competence.nom }}</td>
            </tr>
            <tr>
